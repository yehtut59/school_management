<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="seq_student" model="ir.sequence">
            <field name="name">Student ID</field>
            <field name="code">school.students</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_class_code" model="ir.sequence">
            <field name="name">Class Code</field>
            <field name="code">school.classes</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>
